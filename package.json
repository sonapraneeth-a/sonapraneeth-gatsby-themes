{
  "name": "sonapraneeth-gatsby-themes",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "author": "Sona Praneeth Akula <sonapraneeth.akula@gmail.com>",
  "license": "MIT",
  "scripts": {
    "base-build": "yarn workspace demo-base build",
    "base-clean": "yarn workspace demo-base clean",
    "base-dev": "DEBUG=@sonapraneeth/gatsby-plugin-themed-components:* yarn workspace demo-base dev",
    "base-devw": "set DEBUG=@sonapraneeth/gatsby-plugin-themed-components:* && yarn workspace demo-base dev",
    "bio-build": "yarn workspace demo-bio build",
    "bio-clean": "yarn workspace demo-bio clean",
    "bio-dev": "DEBUG=@sonapraneeth/gatsby-theme-bio:* yarn workspace demo-bio dev",
    "bio-devw": "set DEBUG=@sonapraneeth/gatsby-theme-bio:* && yarn workspace demo-bio dev",
    "project-build": "yarn workspace demo-project build",
    "project-clean": "yarn workspace demo-project clean",
    "project-dev": "DEBUG=@sonapraneeth/gatsby-theme-project:* yarn workspace demo-project dev",
    "project-devw": "set DEBUG=@sonapraneeth/gatsby-theme-project:* && yarn workspace demo-project dev",
    "blog-build": "yarn workspace demo-blog build",
    "blog-clean": "yarn workspace demo-blog clean",
    "blog-dev": "DEBUG=@sonapraneeth/gatsby-theme-blog:* yarn workspace demo-blog dev",
    "blog-devw": "set DEBUG=@sonapraneeth/gatsby-theme-blog:* && yarn workspace demo-blog dev",
    "profile-build": "yarn workspace demo-profile build",
    "profile-clean": "yarn workspace demo-profile clean",
    "profile-dev": "DEBUG=@sonapraneeth/gatsby-theme-profile:* yarn workspace demo-profile dev",
    "profile-devw": "set DEBUG=@sonapraneeth/gatsby-theme-profile:* && yarn workspace demo-profile dev",
    "collection-build": "yarn workspace demo-collection build",
    "collection-clean": "yarn workspace demo-collection clean",
    "collection-dev": "DEBUG=@sonapraneeth/gatsby-theme-collection:* yarn workspace demo-collection dev",
    "collection-devw": "set DEBUG=@sonapraneeth/gatsby-theme-collection:* && yarn workspace demo-collection dev",
    "publication-build": "yarn workspace demo-publication build",
    "publication-clean": "yarn workspace demo-publication clean",
    "publication-dev": "DEBUG=@sonapraneeth/gatsby-theme-publication:* yarn workspace demo-publication dev",
    "publication-devw": "set DEBUG=@sonapraneeth/gatsby-theme-publication:* && yarn workspace demo-publication dev",
    "timeline-build": "yarn workspace demo-timeline build",
    "timeline-clean": "yarn workspace demo-timeline clean",
    "timeline-dev": "DEBUG=@sonapraneeth/gatsby-theme-timeline:* yarn workspace demo-timeline dev",
    "timeline-devw": "set DEBUG=@sonapraneeth/gatsby-theme-timeline:* && yarn workspace demo-timeline dev",
    "all-build": "yarn workspace demo-all build",
    "all-clean": "yarn workspace demo-all clean",
    "all-dev": "DEBUG=@sonapraneeth/*:* yarn workspace demo-all dev",
    "all-devw": "set DEBUG=@sonapraneeth/*:* && yarn workspace demo-all dev",
    "clean": "yarn base-clean && yarn bio-clean && yarn project-clean && yarn blog-clean && yarn all-clean",
    "lc": "lerna clean",
    "lp": "lerna list --json --all --toposort",
    "new-pre-release-version": "lerna version prerelease",
    "new-version": "lerna version",
    "pub": "lerna publish"
  },
  "workspaces": [
    "packages/*",
    "demo/*",
    "test/*"
  ],
  "devDependencies": {
    "commitizen": "^4.0.4",
    "cz-customizable": "^6.2.0",
    "debug": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.5",
    "precise-commits": "^1.0.2",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
